package domain;

import java.util.ArrayList;

public class Unidade {

	private ArrayList<Valor> valor = new ArrayList<>();

	public Unidade() {
		this.valor.add(new Valor(0, "zero"));
		this.valor.add(new Valor(1, "um"));
		this.valor.add(new Valor(2, "dois"));
		this.valor.add(new Valor(3, "três"));
		this.valor.add(new Valor(4, "quatro"));
		this.valor.add(new Valor(5, "cinco"));
		this.valor.add(new Valor(6, "seis"));
		this.valor.add(new Valor(7, "sete"));
		this.valor.add(new Valor(8, "oito"));
		this.valor.add(new Valor(9, "nove"));
	}

	public ArrayList<Valor> getValor() {
		return valor;
	}

	public void setValor(ArrayList<Valor> valor) {
		this.valor = valor;
	}

}
